	set_val(Pickaxe, 0);
	wait(PickaxeRelT + LagCounterT);
	set_val(Pickaxe, 100);
	wait(PickaxePressT + LagCounterT);
	set_val(Pickaxe, 0);
	wait(PickaxeRelT + LagCounterT);
	set_val(Pickaxe, 100);
	wait(PickaxePressT + LagCounterT);
	set_val(Pickaxe, 0);
	wait(PickaxeRelT + LagCounterT);
